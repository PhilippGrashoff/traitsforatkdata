{
  "name": "philippgrashoff/traitsforatkdata",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config":{
    "platform":{
      "php":"7.3"
    }
  },
  "require":{
    "atk4/data": "dev-develop",
    "atk4/ui": "dev-develop",
    "atk4/core": "dev-develop",
    "atk4/schema": "dev-develop",
    "ext-sodium": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">8"
  },
  "autoload":{
    "psr-4":{
      "traitsforatkdata\\":"src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "traitsforatkdata\\tests\\": "tests"
    }
  }
}
